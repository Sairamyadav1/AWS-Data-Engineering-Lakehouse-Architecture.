{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::YOUR_SNOWFLAKE_AWS_ACCOUNT_ID:user/YOUR_SNOWFLAKE_USER"
      },
      "Action": "sts:AssumeRole",
      "Condition": {
        "StringEquals": {
          "sts:ExternalId": "YOUR_SNOWFLAKE_EXTERNAL_ID"
        }
      }
    }
  ]
}

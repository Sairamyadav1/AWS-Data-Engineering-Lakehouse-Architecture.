{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "SnowflakeS3Access",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion",
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::your-gold-bucket/*",
        "arn:aws:s3:::your-gold-bucket",
        "arn:aws:s3:::your-silver-bucket/*",
        "arn:aws:s3:::your-silver-bucket"
      ]
    },
    {
      "Sid": "SnowflakeKMSAccess",
      "Effect": "Allow",
      "Action": [
        "kms:Decrypt",
        "kms:DescribeKey"
      ],
      "Resource": "arn:aws:kms:*:*:key/*",
      "Condition": {
        "StringLike": {
          "kms:ViaService": "s3.*.amazonaws.com"
        }
      }
    }
  ]
}

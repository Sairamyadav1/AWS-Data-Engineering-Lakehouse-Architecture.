{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "SnowflakeS3Access",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion",
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::lakehouse-bucket-name/gold/*",
        "arn:aws:s3:::lakehouse-bucket-name"
      ]
    },
    {
      "Sid": "SnowflakeS3List",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": "arn:aws:s3:::lakehouse-bucket-name",
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "gold/*"
          ]
        }
      }
    }
  ]
}
